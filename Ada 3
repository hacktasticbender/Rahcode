int cost[10][10],n; 

int find(int node, int parent[10]) 

{ 

while(parent[node]!=node) 

node=parent[node]; 

return node; 

}

int union_ij(int i,int j,int parent[10]) 

{ 

if(i<j) 

parent[j]=i; 

else 

parent[i]=j; 

}

